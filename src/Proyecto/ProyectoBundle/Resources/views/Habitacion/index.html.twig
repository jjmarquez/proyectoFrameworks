{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Habitacion list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Numero</th>
                <th>Contnac</th>
                <th>Continter</th>
                <th>Tipo</th>
                <th>Categoria</th>
                <th>Camaind</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('habitacion_show', { 'id': entity.numero }) }}">{{ entity.numero }}</a></td>
                <td>{{ entity.contNac }}</td>
                <td>{{ entity.contInter }}</td>
                <td>{{ entity.tipo }}</td>
                <td>{{ entity.categoria }}</td>
                <td>{{ entity.camaInd }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('habitacion_show', { 'id': entity.numero }) }}">show</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    {% endblock %}
